<Styles
    xmlns="https://github.com/avaloniaui"
    xmlns:system="clr-namespace:System;assembly=netstandard"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Styles.Resources>
        <ResourceDictionary>



            <Color x:Key="SukiBackground">Transparent</Color>
            <Color x:Key="SukiLightBackground">Transparent</Color>
            <system:Boolean x:Key="GlassEnabled">True</system:Boolean>
            <system:Boolean x:Key="GlassDisabled">False</system:Boolean>


            <ResourceDictionary.ThemeDictionaries>
                <ResourceDictionary x:Key="Light">
                    <Color x:Key="SukiText">#222222</Color>
                    <Color x:Key="SukiLowText">#555555</Color>
                    <Color x:Key="SukiCardBackground">#fafafa</Color>
                    <Color x:Key="SukiLightBorderBrush">#f1f1f1</Color>
                    <Color x:Key="SukiControlBorderBrush">#cecece</Color>
                </ResourceDictionary>
                <ResourceDictionary x:Key="Dark">
                    <Color x:Key="SukiText">#edffffff</Color>
                    <Color x:Key="SukiLowText">#caffffff</Color>
                </ResourceDictionary>
            </ResourceDictionary.ThemeDictionaries>
        </ResourceDictionary>
    </Styles.Resources>
</Styles>
