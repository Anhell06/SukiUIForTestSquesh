<Application
    x:Class="MobileDemo.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:local="using:MobileDemo"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Application.DataTemplates>
        <local:ViewLocator />
    </Application.DataTemplates>

    <Application.Styles>
        <StyleInclude Source="avares://SukiUI/Theme/Index.xaml" />
        <StyleInclude Source="avares://Material.Icons.Avalonia/App.xaml" />

    </Application.Styles>
    <Application.Resources>
        <CornerRadius x:Key="SmallCornerRadius">0</CornerRadius>
        <CornerRadius x:Key="MediumCornerRadius">0</CornerRadius>
    </Application.Resources>
</Application>
